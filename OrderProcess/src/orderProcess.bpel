<?xml version="1.0" encoding="UTF-8"?>
<process
    name="orderProcess"
    targetNamespace="http://enterprise.netbeans.org/bpel/OrderProcess/orderProcess"
    xmlns:tns="http://enterprise.netbeans.org/bpel/OrderProcess/orderProcess"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://infosys.tuwien.ac.at/aic10/ass2/OrderProcessSchema" xmlns:ns1="http://infosys.tuwien.ac.at/aic10/dto/supplier" xmlns:ns2="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
    <import namespace="http://infosys.tuwien.ac.at/aic10/ass2/process" location="OrderProcess.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WarehouseWrapper" location="WarehouseWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://infosys.tuwien.ac.at/aic10/dto/warehouse" location="Warehouse.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/Supplier2Wrapper" location="Supplier2Wrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://infosys.tuwien.ac.at/aic10/dto/supplier" location="Supplier2.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/CustomerManagementServiceWrapperWrapper" location="CustomerManagementServiceWrapperWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://infosys.tuwien.ac.at/aic10/dto/custommanagerwrapper" location="CustomerManagementServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/ShippingWrapper" location="ShippingWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://infosys.tuwien.ac.at/aic10/dto/shipping" location="Shipping.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="CustomerPL" xmlns:tns="http://enterprise.netbeans.org/bpel/CustomerManagementServiceWrapperWrapper" partnerLinkType="tns:CustomerManagementServiceWrapperLinkType" partnerRole="CustomerManagementServiceWrapperRole"/>
        <partnerLink name="WarehousePL" xmlns:tns="http://enterprise.netbeans.org/bpel/WarehouseWrapper" partnerLinkType="tns:WarehouseServiceLinkType" partnerRole="WarehouseServiceRole"/>
        <partnerLink name="Supplier2PL" xmlns:tns="http://enterprise.netbeans.org/bpel/Supplier2Wrapper" partnerLinkType="tns:SupplierServiceLinkType" partnerRole="SupplierServiceRole"/>
        <partnerLink name="ShippingPL" xmlns:tns="http://enterprise.netbeans.org/bpel/ShippingWrapper" partnerLinkType="tns:ShippingServiceLinkType" partnerRole="ShippingServiceRole"/>
        <partnerLink name="OrderProcessPL" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" partnerLinkType="tns:OrderProcessLinkType" myRole="OrderProcessProvider"/>
    </partnerLinks>
    <variables>
        <variable name="externalOrderOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" messageType="tns:start_process_response"/>
        <variable name="shipItemsOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/shipping" messageType="tns:ship_itemsResponse"/>
        <variable name="shipItemsIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/shipping" messageType="tns:ship_items"/>
        <variable name="getCustomerOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/custommanagerwrapper" messageType="tns:getResponse">
            <sxed2:editor>
                <sxed2:predicate path="$getCustomerOut.parameters/return/adresses[1]" source="from"/>
            </sxed2:editor>
        </variable>
        <variable name="getCustomerIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/custommanagerwrapper" messageType="tns:get"/>
        <variable name="supplierOrderOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/supplier" messageType="tns:orderResponse"/>
        <variable name="supplierOrderIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/supplier" messageType="tns:order"/>
        <variable name="warehouseOrderOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" messageType="tns:orderResponse"/>
        <variable name="warehouseOrderIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" messageType="tns:order"/>
        <variable name="externalOrderIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" messageType="tns:start_process_request"></variable>
    </variables>
    <sequence>
        <receive name="ReceiveExternalOrder" createInstance="yes" partnerLink="OrderProcessPL" operation="start_process" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" portType="tns:OrderProcessPortType" variable="externalOrderIn"/>
        <assign name="Assign1">
            <copy>
                <from>$externalOrderIn.part1/ns0:customer</from>
                <to>$getCustomerIn.parameters/id</to>
            </copy>
        </assign>
        <invoke name="InvokeGetCustomer" partnerLink="CustomerPL" operation="get" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/custommanagerwrapper" portType="tns:CustomerManagementServiceWrapper" inputVariable="getCustomerIn" outputVariable="getCustomerOut"/>
        <forEach name="ForEach1" parallel="no" counterName="ForEach1Counter">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($externalOrderIn.part1/ns0:orderParts)</finalCounterValue>
            <completionCondition>
                <branches>count($externalOrderIn.part1/ns0:orderParts)</branches>
            </completionCondition>
            <scope name="Scope1">
                <variables>
                    <variable name="checkAvailabilityOut" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" messageType="tns:check_availabilityResponse"/>
                    <variable name="checkAvailabilityIn" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" messageType="tns:check_availability"/>
                </variables>
                <sequence name="Sequence1">
                    <assign name="Assign2">
                        <copy>
                            <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:amount</from>
                            <to>$checkAvailabilityIn.parameters/amount</to>
                        </copy>
                        <copy>
                            <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:product_id
                                <sxed2:editor>
                                    <sxed2:predicate path="$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]" source="from"/>
                                </sxed2:editor>
                            </from>
                            <to>$checkAvailabilityIn.parameters/product/@id</to>
                        </copy>
                    </assign>
                    <invoke name="InvokeCheckAvialability" partnerLink="WarehousePL" operation="check_availability" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" portType="tns:WarehouseService" inputVariable="checkAvailabilityIn" outputVariable="checkAvailabilityOut"/>
                    <if name="If1">
                        <condition>$checkAvailabilityOut.parameters/return/isAvailable</condition>
                        <sequence name="Sequence2">
                            <assign name="Assign3">
                                <copy>
                                    <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:amount
                                        <sxed2:editor>
                                            <sxed2:predicate path="$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]" source="from"/>
                                        </sxed2:editor>
                                    </from>
                                    <to>$warehouseOrderIn.parameters/amount</to>
                                </copy>
                                <copy>
                                    <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:product_id
                                        <sxed2:editor>
                                            <sxed2:predicate path="$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]" source="from"/>
                                        </sxed2:editor>
                                    </from>
                                    <to>$warehouseOrderIn.parameters/product/@id</to>
                                </copy>
                            </assign>
                            <invoke name="InvokeWarehouseOrder" partnerLink="WarehousePL" operation="order" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/warehouse" portType="tns:WarehouseService" inputVariable="warehouseOrderIn" outputVariable="warehouseOrderOut"/>
                        </sequence>
                        <else>
                            <sequence name="Sequence3">
                                <assign name="Assign4">
                                    <copy>
                                        <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:amount
                                            <sxed2:editor>
                                                <sxed2:predicate path="$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]" source="from"/>
                                            </sxed2:editor>
                                        </from>
                                        <to>$supplierOrderIn.parameters/amount</to>
                                    </copy>
                                    <copy>
                                        <from>$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]/ns0:product_id
                                            <sxed2:editor>
                                                <sxed2:predicate path="$externalOrderIn.part1/ns0:orderParts[$ForEach1Counter]" source="from"/>
                                            </sxed2:editor>
                                        </from>
                                        <to>$supplierOrderIn.parameters/product/@id</to>
                                    </copy>
                                </assign>
                                <invoke name="InvokeSupplierOrder" partnerLink="Supplier2PL" operation="order" portType="ns1:SupplierService" inputVariable="supplierOrderIn" outputVariable="supplierOrderOut"/>
                            </sequence>
                        </else>
                    </if>
                </sequence>
            </scope>
        </forEach>
        <flow name="Flow1">
            <sequence name="FlowSequence">
                <assign name="Assign5">
                    <copy>
                        <from>$getCustomerOut.parameters/return/adresses[1]
                            <sxed2:editor>
                                <sxed2:predicate path="$getCustomerOut.parameters/return/adresses[1]" source="from"/>
                            </sxed2:editor>
                        </from>
                        <to>$shipItemsIn.parameters/address</to>
                    </copy>
                    <copy>
                        <from>ns2:doXslTransform('urn:stylesheets:OrderItemTransformation.xsl', $externalOrderIn.part1/ns0:orderParts)</from>
                        <to>$shipItemsIn.parameters/items</to>
                    </copy>
                </assign>
                <invoke name="InvokeShipItems" partnerLink="ShippingPL" operation="ship_items" xmlns:tns="http://infosys.tuwien.ac.at/aic10/dto/shipping" portType="tns:ShippingService" inputVariable="shipItemsIn" outputVariable="shipItemsOut"/>
            </sequence>
        </flow>
        <assign name="Assign6">
            <copy>
                <from>$shipItemsOut.parameters/return</from>
                <to>$externalOrderOut.part1/ns0:orderResult</to>
            </copy>
        </assign>
        <reply name="ReplyExternalOrder" partnerLink="OrderProcessPL" operation="start_process" xmlns:tns="http://infosys.tuwien.ac.at/aic10/ass2/process" portType="tns:OrderProcessPortType" variable="externalOrderOut"/>
    </sequence>
</process>




